<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1">
    <children>
        <VBox alignment="CENTER" prefHeight="100.0" prefWidth="100.0" spacing="30.0">
            <children>
                <Text fx:id="okText" strokeType="OUTSIDE" strokeWidth="0.0" text="BOTTOM TEXT"/>
                <Button fx:id="okButton" mnemonicParsing="false" onMouseClicked="#closeWindow" text="OK"/>
            </children>
            <StackPane.margin>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </StackPane.margin>
        </VBox>
    </children>
</StackPane>
